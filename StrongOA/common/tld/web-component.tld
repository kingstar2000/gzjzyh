	<!-- 日期时间组件标签 -->
	<tag>
		<name>datetime</name>
		<tag-class>
			com.strongit.platform.webcomponent.datetime.DateTime
		</tag-class>
		<body-content>jsp</body-content>
		<description>
			<![CDATA[日期时间组件]]>
		</description>


		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[对应input的name属性，如果没有指定，则和name属性相同]]>
			</description>
		</attribute>

		<attribute>
			<name>format</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[日期展示的格式，默认为'yyyy-MM-dd',其他值，要显示时间 yyyy-MM-dd HH:mm:ss]]>
			</description>
		</attribute>

		<attribute>
			<name>readOnly</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[readOnly为true表示不可以直接修改]]>
			</description>
		</attribute>

		<attribute>
			<name>validator</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[验证器]]>
			</description>
		</attribute>
	</tag>

	<tag>
		<name>inlinedatetime</name>
		<tag-class>
			com.strongit.platform.webcomponent.datetime.InlineDateTime
		</tag-class>
		<body-content>jsp</body-content>
		<description>
			<![CDATA[内联日期时间组件]]>
		</description>

		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[对应input的name属性，如果没有指定，则和name属性相同]]>
			</description>
		</attribute>

		<attribute>
			<name>format</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[日期展示的格式，默认为'yyyy-MM-dd',其他值，要显示时间 yyyy-MM-dd HH:mm:ss]]>
			</description>
		</attribute>

		<!-- 
			<attribute>
			<name>lang</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[指定弹出组件的语言，可以选择的值有auto,cn,en.不设置默认为auto表示更具浏览器来判断语言,cn表示中文,en表示英文]]></description>
			</attribute>
		-->
	</tag>

	<!-- 表单验证组件标签 -->
	<tag>
		<name>validator</name>
		<tag-class>
			com.strongit.platform.webcomponent.validator.ValidatorTag
		</tag-class>
		<body-content>JSP</body-content>
		<description>
			<![CDATA[表单验证组件标签]]>
		</description>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[表单验证器名称]]>
			</description>
		</attribute>

		<attribute>
			<name>errorDisplayContainer</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[当使用tips形式展现错误信息时错误信息的容器Id]]>
			</description>
		</attribute>

		<attribute>
			<name>errorClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[错误样式名]]>
			</description>
		</attribute>

		<attribute>
			<name>tipClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[采用tip信息展现方式时，验证错误提示信息主体样式名称]]>
			</description>
		</attribute>

		<attribute>
			<name>errorElement</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[错误提示信息容器名称]]>
			</description>
		</attribute>

		<attribute>
			<name>errorContainer</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[当使用普通形式展现错误信息时，整体错误提示信息容器Id]]>
			</description>
		</attribute>

		<attribute>
			<name>errorLabelContainer</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[当使用普通形式展现错误信息时，单个错误提示信息容器Id]]>
			</description>
		</attribute>

		<attribute>
			<name>wrapper</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[单个提示信息的包装容器名称]]>
			</description>
		</attribute>

		<attribute>
			<name>formId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[要验证的表单Id]]>
			</description>
		</attribute>

		<attribute>
			<name>submitTip</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[表单验证通过提交时是否展示提示信息,依赖于common.js]]>
			</description>
		</attribute>
	</tag>

