	<!-- 导航菜单组件标签 -->
	<tag>
		<name>navigationMenu</name>
		<tag-class>
			com.strongit.platform.webcomponent.verticalnavigation.VerticalNavigation
		</tag-class>
		<body-content>jsp</body-content>
		<description>
			<![CDATA[垂直导航菜单]]>
		</description>

		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[id]]>
			</description>
		</attribute>

		<attribute>
			<name>nodes</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[服务器端数据]]>
			</description>
		</attribute>

		<attribute>
			<name>jsondata</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[json数据]]>
			</description>
		</attribute>

		<attribute>
			<name>wholeCss</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[json数据]]>
			</description>
		</attribute>

		<attribute>
			<name>hideIcons</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[当header下没有子级菜单时是否隐藏header前面的图标，当icons不为false时有效，默认为false]]>
			</description>
		</attribute>

		<attribute>
			<name>hideDiv</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[当header下没有子级菜单时点击header时是否展现空白的子级div，当fillSpace为true时有效，默认为false]]>
			</description>
		</attribute>

		<attribute>
			<name>headerNavigation</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[点击header是否能触发链接事件，默认为false]]>
			</description>
		</attribute>

		<attribute>
			<name>fillSpace</name>
			<required>true</required>
