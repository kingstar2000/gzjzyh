<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_2_3.xsd">

	<tlib-version>2.2.3</tlib-version>
	<short-name>/tags-web/component</short-name>
	<description>
		<![CDATA["web组件标签"]]>
	</description>

	<!-- 自动完成组件标签 -->
	<tag>
		<name>autocomplete</name>
		<tag-class>
			com.strongit.platform.webcomponent.autocomplete.AutoComplete
		</tag-class>
		<body-content>jsp</body-content>
		<description>
			<![CDATA[自动完成]]>
		</description>

		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[选择框的 class]]>
			</description>
		</attribute>

		<attribute>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[data]]>
			</description>
		</attribute>

		<attribute>
			<name>data</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[data]]>
			</description>
		</attribute>
	</tag>

	<!-- 级联选择组件标签 -->
	<tag>
		<name>Cascade</name>
		<tag-class>
			com.strongit.platform.webcomponent.cascade.Cascade
		</tag-class>
		<body-content>jsp</body-content>
		<description>
			<![CDATA[级联菜单]]>
		</description>

		<attribute>
			<name>selclass</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[选择框的 class]]>
			</description>
		</attribute>

		<attribute>
			<name>data</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[data]]>
			</description>
		</attribute>

		<attribute>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[local or remote]]>
			</description>
		</attribute>
	</tag>

	<!-- 菜单组件标签 -->
	<tag>
		<name>menu</name>
		<tag-class>
			com.strongit.platform.webcomponent.contextmenu.ContextMenuTag
		</tag-class>
		<body-content>JSP</body-content>
		<description>
			<![CDATA[右键菜单组件主标签]]>
		</description>
		<attribute>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[右键菜单名称]]>
			</description>
		</attribute>

		<attribute>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[菜单宽度，默认为150px]]>
			</description>
		</attribute>

		<attribute>
			<name>theme</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[菜单主体样式名称]]>
			</description>
		</attribute>

		<attribute>
			<name>onShow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[右键菜单展现前调用的脚本函数名称，但对右键菜单是否展现不产生影响]]>
			</description>
		</attribute>

		<attribute>
			<name>onContextMenu</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[右键菜单展现前调用的脚本函数名称，若返回false则右键菜单不展现]]>
			</description>
		</attribute>
	</tag>

	<tag>
		<name>menuGroup</name>
		<tag-class>
			com.strongit.platform.webcomponent.contextmenu.ContextMenuGroupTag
		</tag-class>
		<body-content>JSP</body-content>
		<description>
			<![CDATA[右键菜单组件组菜单项标签]]>
		</description>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[组菜单项名称]]>
			</description>
		</attribute>

		<attribute>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[组菜单下的子菜单宽度，默认为150px]]>
			</description>
		</attribute>

		<attribute>
			<name>icon</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[组菜单项图片地址]]>
			</description>
		</attribute>

		<attribute>
			<name>text</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[组菜单项名称]]>
			</description>
		</attribute>

		<attribute>
			<name>action</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				<![CDATA[组菜单项点击时触发的脚本函数名称]]>
			</description>
		</attribute>
	</tag>

